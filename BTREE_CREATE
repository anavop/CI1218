
CREATE INDEX NATION_INDEX_BTREE ON Nation(N_NATIONKEY,N_REGIONKEY,N_NAME)
CREATE INDEX REGION_INDEX_BTREE ON REGION(R_REGIONKEY,R_NAME)
CREATE INDEX CUSTOMER_INDEX_BTREE ON CUSTOMER(C_CUSTKEY,C_NATIONKEY,C_NAME)
CREATE INDEX PART_INDEX_BTREE ON PART(P_PARTKEY,P_NAME)
CREATE INDEX SUPPLIER_INDEX_BTREE ON SUPPLIER(S_SUPPKEY,S_NAME,S_NATIONKEY)
CREATE INDEX PARTSUPP_INDEX_BTREE ON PARTSUPP(PS_SUPPKEY, PS_PARTKEY)
CREATE INDEX ORDERS_INDEX_BTREE ON ORDERS(O_ORDERKEY,O_CUSTKEY,O_TOTALPRICE)
CREATE INDEX LINEITEM_INDEX_BTREE ON LINEITEM(L_ORDERKEY,L_PARTKEY,L_SUPPKEY)



DROP INDEX NATION_INDEX_BTREE 
DROP INDEX REGION_INDEX_BTREE
DROP INDEX CUSTOMER_INDEX_BTREE 
DROP INDEX PART_INDEX_BTREE
DROP INDEX SUPPLIER_INDEX_BTREE 
DROP INDEX PARTSUPP_INDEX_BTREE 
DROP INDEX ORDERS_INDEX_BTREE 
DROP INDEX LINEITEM_INDEX_BTREE 
